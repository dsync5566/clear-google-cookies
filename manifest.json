{
  "name": "Clear Google Cookies",
  "description": "Delete Google cookies to fix Youtube or Google Maps",
  "version": "1.0",
  "permissions": [ "cookies", "activeTab", "*://*/*" ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
